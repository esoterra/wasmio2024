import { level, log } from wasi:logging/logging;

import foo: func(a: string) -> string;

export func run() {
    log(level::info, "plugin", "Plugin started");

    log(level::debug, "plugin", "Calling foo(\"a\")");
    foo("a");

    log(level::debug, "plugin", "Calling foo(\"slow-input\")");
    foo("slow-input");
    
    log(level::info, "plugin", "Plugin finished");
}